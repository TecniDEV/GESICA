    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <SignAssembly>False</SignAssembly>
    <Authors>TecniDEV</Authors>
    <Product>GESICA</Product>
    <Description>Gestor Empresarial de Informaci√≥n Contable y Administrativa</Description>
    <Copyright>CC0-1.0</Copyright>
